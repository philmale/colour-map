{#- Phil: Macros for colour names -#}
{#- NB: Macros return strings, so results are json format -#}
{#- LA-HA v3.0 -#}

{#-
from 'colour_map.jinja' import
  random_colour,
  random_colours,
  sunset_colour,
  lookup_colour,
  lookup_colours,
  find_colour,
  average_hex_colour,
  nearest_colour_name,
  hex_complementary,
  hex_complementary_named,
  complementary_colours,
  complementary_colours_named,
  hex2rgb,
  hex2hsv,
  hex2hsl,
  hex2hs,
  hex2h,
  hex2xyb,
  hsv2rgb,
  hsv2hex,
  rgb2hex
-#}

{#--
 # Random Colour Selection
 #
 # These macros help you select random colours from named colour
 # maps. Three maps are available: "lamp" (general lighting),
 # "f1" (Formula 1 team colours), and "ha" (Home Assistant colours).
 # Others can be added.
--#}

{#-
 #  Select a single random colour from a colour map
 #
 #  Parameters:
 #    map: Colour name map to use (default: "lamp")
 #         Options: "lamp", "f1", "ha"
 #
 #  Returns:
 #    JSON string containing the colour name.
 #
 #  Examples:
 #    {{ random_colour() | from_json }}
 #    Returns: "Forest Green"
 #
 #    {{ random_colour("f1")  | from_json }}
 #    Returns: "Ferrari Red"
-#}
{%- macro random_colour(map="lamp") -%}
  {%- set colour_map = colours[map] -%}
  {{- colour_map.keys() | list | random | to_json -}}
{%- endmacro -%}

{#-
 #  Select multiple random colours without repeats
 #
 #  Parameters:
 #    n: Number of colours to return (default: 1)
 #    map: Colour name map to use (default: "lamp")
 #         Options: "lamp", "f1", "ha"
 #
 #  Returns:
 #    JSON array of colour names without duplicates.
 #    If n exceeds available colours, returns all colours.
 #
 #  Examples:
 #    {{ random_colours(3) | from_json }}
 #    Returns: ["Forest Green", "Crimson Red", "Ocean Blue"]
 #
 #    {{ random_colours(5, "f1") | from_json }}
 #    Returns: Five different F1 team colours
-#}
{%- macro random_colours(n=1, map="lamp") -%}
  {%- set colour_map = colours[map] -%}
  {%- set all_colours = colour_map.keys() | list -%}
  {%- set capped_n = [n, all_colours | count] | min -%}
  {%- set result = namespace(colours=[]) -%}
  {%- for i in range(capped_n) -%}
    {%- set available = all_colours | reject('in', result.colours) | list -%}
    {%- set picked = available | random -%}
    {%- set result.colours = result.colours + [picked] -%}
  {%- endfor -%}
  {{- result.colours | to_json -}}
{%- endmacro -%}

{#-
 # Lookup a random colour that is not too red or white
 # Handy for outside light colour settings...
 #
 # Parameters:
 #    map: Colour map to search (default: "lamp")
 #         Options: "lamp", "f1", "ha"
 # Returns:
 #   JSON format string name of the colour selected.
 # 
 # Examples:
 #   {{ sunset_colour() | from_json }}
 #   Returns: "Forest Green"
-#}
{%- macro sunset_colour(map) -%}
  {%- set colour_map = colours[map] -%}
  {%- set keys = colour_map.keys() | list -%}
  {%- set max_tries = keys | count -%}
  {%- set candidate = namespace(c = "") -%}
  {%- for i in range(max_tries) -%}
    {%- set candidate.c = keys | random -%}
    {%- set (h,s) = hex2hs(colour_map[candidate.c]) | from_json -%}
    {%- set is_white = s < 5 -%}
    {%- set is_red = h > 350 or h < 10 -%}
    {%- if not is_white and not is_red -%}
      {%- break -%}
    {%- endif -%}
  {%- endfor -%}
  {{ candidate.c | to_json }}
{%- endmacro -%}

{#--
 # Colour Lookup and Search
 #
 # Look up colours by name and retrieve them in various
 # formats (name, hex, rgb, hsv, hsl, hs, h, xyb). These macros
 # search the colour maps and return the requested format.
--#}

{#-
 #  Look up a colour and return it in the requested format
 #
 #  Parameters:
 #    id: Colour name (default: None returns random)
 #        Special value "random" also returns random
 #        Special value "sunset_safe" returns not red or white(!)
 #    attr: Format to return (default: "name")
 #          Options: "name", "hex", "rgb", "hsv", "hsl", "hs",
 #                   "h", "xyb"
 #    map: Colour map to search (default: "lamp")
 #         Options: "lamp", "f1", "ha"
 #
 #  Returns:
 #    JSON value in the requested format, or empty if not found.
 #
 #  Examples:
 #    {{ lookup_colour("Forest Green", "hex") | from_json }}
 #    Returns: "#228B22"
 #
 #    {{ lookup_colour("Ferrari Red", "hs", "f1") | from_json }}
 #    Returns: [0, 100]
 #
 #    {{ lookup_colour("random") | from_json }}
 #    Returns: Random colour name
-#}
{%- macro lookup_colour(id=None, attr="name", map="lamp") -%}
  {%- set colour_map = colours[map] -%}
  {%- if id is none or id == "random" -%}
    {%- set id = random_colour(map) | from_json -%}
  {%- elif id == 'sunset_safe' -%}
    {%- set id = sunset_colour(map) | from_json -%}
  {%- else -%}
    {%- set id = find_colour(id, map) | from_json -%}
  {%- endif -%}
  {%- if id is not none -%}
    {%- if attr == "rgb" -%}
      {{- hex2rgb(colour_map[id]) -}}
    {%- elif attr == "hsv" -%}
      {{- hex2hsv(colour_map[id]) -}}
    {%- elif attr == "hs" -%}
      {{- hex2hs(colour_map[id]) -}}
    {%- elif attr == "h" -%}
      {{- hex2h(colour_map[id]) -}}
    {%- elif attr == "hsl" -%}
      {{- hex2hsl(colour_map[id]) -}}
    {%- elif attr == "xyb" -%}
      {{- hex2xyb(colour_map[id]) -}}
    {%- elif attr == "hex" -%}
      {{- ('#' + colour_map[id]) | to_json -}}
    {%- else -%}
      {{- id | to_json -}}
    {%- endif -%}
  {%- else -%}
    {{ None | to_json }}
  {%- endif -%}
{%- endmacro -%}

{#-
 #  Look up multiple colours and return them in the requested
 #  format
 #
 #  Parameters:
 #    cs: Colour name(s) - can be:
 #        - None (returns random)
 #        - String (single colour or comma-separated list)
 #        - List of colour names
 #    attr: Format to return (default: "name")
 #          Options: "name", "hex", "rgb", "hsv", "hsl", "hs",
 #                   "h", "xyb"
 #    map: Colour map to search (default: "lamp")
 #         Options: "lamp", "f1", "ha"
 #
 #  Returns:
 #    JSON array of values in the requested format.
 #
 #  Examples:
 #    {{ lookup_colours("Red, Green and Blue", "hs") | from_json }}
 #    Returns: [[0, 100], [120, 100], [240, 100]]
 #
 #    {{ lookup_colours("Red, Green, and Blue", "hs") | from_json }}
 #    Returns: [[0, 100], [120, 100], [240, 100]]
 #
 #    {{ lookup_colours(["Ferrari Red", "Mercedes Blue"],
 #                      "hex", "f1") | from_json }}
 #    Returns: ["#FF2800", "#27F4D2"]
-#}
{%- macro lookup_colours(cs=None, attr="name", map="lamp") -%}
  {%- set r = namespace(c=[]) -%}
  {%- if cs is none -%}
    {%- set cs = random_colour(map) | from_json -%}
  {%- endif -%}
  {%- if cs is string -%}
    {%- set cs = (cs | regex_replace('\\s*,?\\s+and\\s+', ',') | 
        regex_replace('\\s*,\\s*', ',')).split(',') | 
        map('trim') | list -%}
  {%- endif -%}
  {%- for i in cs -%}
    {%- set lu = lookup_colour(i, attr, map) | from_json -%}
    {%- if lu -%}
      {%- set r.c = r.c + [lu] -%}
    {%- endif -%}
  {%- endfor -%}
  {{- r.c | to_json -}}
{%- endmacro -%}

{#-
 #  Find a colour by name (case-insensitive search)
 #
 #  Parameters:
 #    name: Colour name to search for (default: None, random)
 #    map: Colour map to search (default: "lamp")
 #         Options: "lamp", "f1", "ha"
 #
 #  Returns:
 #    JSON string with the matching colour tag, or empty if not
 #    found.
 #
 #  Examples:
 #    {{ find_colour("forest green") | from_json }}
 #    Returns: "Forest Green"
 #
 #    {{ find_colour("FERRARI RED", "f1") | from_json }}
 #    Returns: "Ferrari Red"
-#}
{%- macro find_colour(name=None, map="lamp") -%}
  {%- set r = namespace(v=None) -%}
  {%- set colour_map = colours[map] -%}
  {%- if name is none -%}
    {%- set r.v = random_colour(map) | from_json -%}
  {%- else -%}
    {%- set name = name|lower -%}
    {%- for i in colour_map.keys() | list -%}
      {%- if i|lower == name -%}
        {%- set r.v = i -%}
        {%- break -%}
      {%- endif -%}
    {%- endfor -%}
  {%- endif -%}
  {{ r.v | to_json }}
{%- endmacro -%}

{#-
 #  Find the nearest named colour for a given hex colour
 #  Thanks to @dabalroman!
 #  Parameters:
 #    hex: Input colour in hex format. Accepts:
 #         - "#RRGGBB"
 #         - "RRGGBB"
 #         - "#RRGGBBAA" (alpha is ignored)
 #         - "RRGGBBAA"  (alpha is ignored)
 #    map: Colour map to search (default: "lamp")
 #         Options: "lamp", "f1", "ha"
 #
 #  Returns:
 #    JSON string with the human-readable colour name of the
 #    closest match from the selected map (e.g. "Forest Green").
 #    If the map is empty or input is invalid, result may be null
 #    (depending on calling context).
 #
 #  Notes:
 #    - Uses RGB Euclidean distance (squared) to find the closest
 #      colour in the map.
 #    - Alpha channel (AA) is ignored if present.
 #
 #  Examples:
 #    {{ nearest_colour_name("#FFF144FF") | from_json }}
 #    Returns: "Jedi Yellow" (closest match in "lamp" map)
 #    
 #    {{ nearest_colour_name("#27F4D2", "f1") | from_json }}
 #    Returns: Closest F1 team colour name - "Mercedes Blue"
-#}
{%- macro nearest_colour_name(hex, map="lamp") -%}
  {%- set colour_map = colours[map] -%}

  {#- Normalize input hex -#}
  {%- set clean_hex = hex.replace('#', '')[0:6].lower() -%}

  {%- set rgb = hex2rgb(clean_hex) | from_json -%}
  {%- set r = rgb[0] -%}
  {%- set g = rgb[1] -%}
  {%- set b = rgb[2] -%}

  {%- set ns = namespace(best=None, best_d=999999999) -%}
  {%- for name in colour_map.keys() | list -%}
    {%- set prgb = hex2rgb(colour_map[name]) | from_json -%}
    {%- set d = (r-prgb[0])**2 + (g-prgb[1])**2 + (b-prgb[2])**2 -%}
    {%- if d < ns.best_d -%}
      {%- set ns.best_d = d -%}
      {%- set ns.best = name -%}
    {%- endif -%}
  {%- endfor -%}

  {{- ns.best | to_json -}}
{%- endmacro -%}

{#- 
 #  Average multiple colours to return a single hex colour
 #  
 #  Parameters:
 #    cs: Colour name(s) - can be:
 #        - None (returns random colour)
 #        - String (single colour or comma-separated list)
 #        - List of colour names
 #    map: Colour map to search (default: "lamp")
 #         Options: "lamp", "f1", "ha"
 #  
 #  Returns: 
 #    JSON string with averaged hex colour (#RRGGBB).
 #    If input is empty or invalid, returns amber colour.
 #  
 #  Examples:
 #    {{ average_hex_colour("Red, Green, Blue") | from_json }}
 #    Returns: "#555555" (average of red, green, blue)
 #
 #    {{ average_hex_colour(["Ferrari Red", "Mercedes Blue"], 
 #                       "f1") | from_json }}
 #    Returns: Averaged hex of Ferrari Red and Mercedes Blue
-#}
{%- macro average_hex_colour(cs=None, map="lamp") -%}
  {#- Get hex colours -#}
  {%- set hex_list = lookup_colours(cs, "hex", map) | 
      from_json -%}
  
  {#- If no valid colours found, return amber colour -#}
  {%- if hex_list | count == 0 -%}
    {{- '#ffbf00' | to_json -}}
  {%- else -%}
    {#- Initialise accumulators for RGB components -#}
    {%- set rgb_sum = namespace(r=0, g=0, b=0) -%}
    
    {#- Sum all RGB values -#}
    {%- for hex_val in hex_list -%}
      {%- set rgb = hex2rgb(hex_val) | from_json -%}
      {%- set rgb_sum.r = rgb_sum.r + rgb[0] -%}
      {%- set rgb_sum.g = rgb_sum.g + rgb[1] -%}
      {%- set rgb_sum.b = rgb_sum.b + rgb[2] -%}
    {%- endfor -%}
    
    {#- Calculate averages and convert to integers -#}
    {%- set count = hex_list | count -%}
    {%- set avg_r = (rgb_sum.r / count) | round(0) | int -%}
    {%- set avg_g = (rgb_sum.g / count) | round(0) | int -%}
    {%- set avg_b = (rgb_sum.b / count) | round(0) | int -%}
    
    {#- Convert back to hex using existing rgb2hex macro -#}
    {{- rgb2hex([avg_r, avg_g, avg_b]) -}}
  {%- endif -%}
{%- endmacro -%}

{#--
 # Complementary Colour Generation
 #
 # Generate harmonious colour palettes from a base colour using
 # various colour theory algorithms.
 #
 # Core macros work with hex values, wrapper macros handle colour
 # names.
--#}

{#-
 #  Generate complementary hex colours from a base hex colour
 #
 #  Parameters:
 #    hex: Input hex colour (with or without #)
 #         e.g., "#FF2800", "FF2800", "#ff2800ff" (alpha ignored)
 #    mode: Harmony algorithm (default: "complementary")
 #          Options:
 #            - "complementary": Opposite on colour wheel (180°)
 #            - "split_complementary": Two colours flanking (150°, 210°)
 #            - "analogous": Adjacent hues (±30°)
 #            - "triadic": Three evenly spaced (120° apart)
 #            - "tetradic": Four rectangularly spaced (60°, 180°, 240°)
 #            - "square": Four evenly spaced (90° apart)
 #
 #  Returns:
 #    JSON array of hex colour strings (including the base colour).
 #    Always returns lowercase hex with # prefix.
 #
 #  Examples:
 #    {{ hex_complementary("#FF0000", "complementary") | from_json }}
 #    Returns: ["#ff0000", "#00ffff"] (red + cyan)
 #
 #    {{ hex_complementary("0000FF", "triadic") | from_json }}
 #    Returns: ["#0000ff", "#00ff00", "#ff0000"] (blue, green, red)
 #
 #    {{ hex_complementary("#FF8000", "split_complementary") | from_json }}
 #    Returns: ["#ff8000", "#0080ff", "#00ff80"] (orange + blues/cyans)
-#}
{%- macro hex_complementary(hex, mode="complementary") -%}
  {#- Normalize input hex -#}
  {%- set clean_hex = hex.replace('#', '')[0:6].lower() -%}

  {#- Get HSV of base colour -#}
  {%- set hsv = hex2hsv(clean_hex) | from_json -%}
  {%- set h = hsv[0] -%}
  {%- set s = hsv[1] -%}
  {%- set v = hsv[2] -%}

  {#- Generate palette based on mode -#}
  {%- set palette = namespace(colours=[]) -%}

  {%- if mode == "complementary" -%}
    {#- Base + opposite (180°) -#}
    {%- set palette.colours = [
      [h, s, v],
      [(h + 180) % 360, s, v]
    ] -%}

  {%- elif mode == "split_complementary" -%}
    {#- Base + two flanking opposite -#}
    {%- set palette.colours = [
      [h, s, v],
      [(h + 150) % 360, s, v],
      [(h + 210) % 360, s, v]
    ] -%}

  {%- elif mode == "analogous" -%}
    {#- Base + adjacent hues -#}
    {%- set palette.colours = [
      [(h - 30) % 360, s, v],
      [h, s, v],
      [(h + 30) % 360, s, v]
    ] -%}

  {%- elif mode == "triadic" -%}
    {#- Three evenly spaced -#}
    {%- set palette.colours = [
      [h, s, v],
      [(h + 120) % 360, s, v],
      [(h + 240) % 360, s, v]
    ] -%}

  {%- elif mode == "tetradic" -%}
    {#- Four rectangularly spaced -#}
    {%- set palette.colours = [
      [h, s, v],
      [(h + 60) % 360, s, v],
      [(h + 180) % 360, s, v],
      [(h + 240) % 360, s, v]
    ] -%}

  {%- elif mode == "square" -%}
    {#- Four evenly spaced -#}
    {%- set palette.colours = [
      [h, s, v],
      [(h + 90) % 360, s, v],
      [(h + 180) % 360, s, v],
      [(h + 270) % 360, s, v]
    ] -%}

  {%- else -%}
    {#- Default to complementary -#}
    {%- set palette.colours = [
      [h, s, v],
      [(h + 180) % 360, s, v]
    ] -%}
  {%- endif -%}

  {#- Convert HSV palette to hex using existing macros -#}
  {%- set result = namespace(hexes=[]) -%}
  {%- for colour_hsv in palette.colours -%}
    {%- set hex_colour = hsv2hex(colour_hsv) | from_json -%}
    {%- set result.hexes = result.hexes + [hex_colour] -%}
  {%- endfor -%}

  {{- result.hexes | to_json -}}
{%- endmacro -%}

{#-
 #  Generate complementary colours from a named colour
 #
 #  Parameters:
 #    colour: Colour name to look up (e.g., "Ferrari Red", "Blue")
 #    mode: Harmony algorithm (default: "complementary")
 #          See hex_complementary for mode options
 #    map: Colour map to search (default: "lamp")
 #
 #  Returns:
 #    JSON array of hex colour strings (including the base colour).
 #
 #  Examples:
 #    {{ complementary_colours("Ferrari Red", "complementary", "f1") | from_json }}
 #    Returns: ["#ff2800", "#00d8ff"] (Ferrari red + cyan)
 #
 #    {{ complementary_colours("Blue", "triadic", "ha") | from_json }}
 #    Returns: Three hex colours in triadic harmony
-#}
{%- macro complementary_colours(colour, mode="complementary", map="lamp") -%}
  {#- Look up hex value for colour name -#}
  {%- set hex = lookup_colour(colour, "hex", map) | from_json -%}
  
  {#- Generate complementary palette -#}
  {{- hex_complementary(hex, mode) -}}
{%- endmacro -%}

{#-
 #  Generate nearest named complementary colours from hex input
 #
 #  Parameters:
 #    hex: Input hex colour (with or without #)
 #    mode: Harmony algorithm (default: "complementary")
 #          See hex_complementary for mode options
 #    map: Colour map to use for naming (default: "lamp")
 #
 #  Returns:
 #    JSON array of colour names nearest to the calculated palette.
 #
 #  Examples:
 #    {{ hex_complementary_named("#FF0000", "complementary", "ha") | from_json }}
 #    Returns: ["Red", "Cyan"] (or nearest HA colour names)
 #
 #    {{ hex_complementary_named("0000FF", "triadic", "f1") | from_json }}
 #    Returns: Three F1 colour names nearest to triadic palette
-#}
{%- macro hex_complementary_named(hex, mode="complementary", map="lamp") -%}
  {#- Generate hex palette -#}
  {%- set hex_palette = hex_complementary(hex, mode) | from_json -%}

  {#- Convert each hex to nearest named colour -#}
  {%- set result = namespace(names=[]) -%}
  {%- for hex_colour in hex_palette -%}
    {%- set name = nearest_colour_name(hex_colour, map) | from_json -%}
    {%- set result.names = result.names + [name] -%}
  {%- endfor -%}

  {{- result.names | to_json -}}
{%- endmacro -%}

{#-
 #  Generate nearest named complementary colours from a named colour
 #
 #  Parameters:
 #    colour: Colour name to look up (e.g., "Ferrari Red")
 #    mode: Harmony algorithm (default: "complementary")
 #          See hex_complementary for mode options
 #    map: Colour map to use (default: "lamp")
 #         Used both for input lookup and output naming
 #
 #  Returns:
 #    JSON array of colour names nearest to the calculated palette.
 #
 #  Examples:
 #    {{ complementary_colours_named("Ferrari Red", "complementary", "f1") | from_json }}
 #    Returns: ["Ferrari Red", "Mercedes Blue"] (or nearest matches)
 #
 #    {{ complementary_colours_named("Blue", "triadic", "ha") | from_json }}
 #    Returns: Three HA colour names in triadic harmony
-#}
{%- macro complementary_colours_named(colour, mode="complementary", map="lamp") -%}
  {#- Look up hex value for colour name -#}
  {%- set hex = lookup_colour(colour, "hex", map) | from_json -%}
  
  {#- Generate named palette -#}
  {{- hex_complementary_named(hex, mode, map) -}}
{%- endmacro -%}

{#--
 # Colour Format Conversion
 #
 # Convert between different colour formats. All conversions work
 # from hex format to other formats, plus rgb to hex.
 # Formats: hex (#RRGGBB), rgb ([r,g,b] 0-255),
 #          hsv ([h,s,v] h:0-360 s,v:0-100),
 #          hsl ([h,s,l] h:0-360 s,l:0-100),
 #          hs ([h,s] hue and saturation only),
 #          h ([h] hue only),
 #          xyb ([x,y,br] CIE coordinates for Home Assistant)
--#}

{#-
 #  Convert hex colour to RGB
 #
 #  Parameters:
 #    hex: Hex colour string (with or without #)
 #
 #  Returns:
 #    JSON array [r, g, b] with values 0-255.
 #
 #  Examples:
 #    {{ hex2rgb("#FF0000") | from_json }}
 #    Returns: [255, 0, 0]
 #
 #    {{ hex2rgb("00FF00") | from_json }}
 #    Returns: [0, 255, 0]
-#}
{%- macro hex2rgb(hex) -%}
  {%- set hex = hex.replace('#','') -%}
  {{- [
      hex[0:2]|int(0,16),
      hex[2:4]|int(0,16),
      hex[4:6]|int(0,16) ] | to_json -}}
{%- endmacro -%}

{#-
 #  Convert RGB colour to hex
 #
 #  Parameters:
 #    rgb: RGB array [r, g, b] with values 0-255
 #
 #  Returns:
 #    JSON string with hex colour (#RRGGBB).
 #
 #  Examples:
 #    {{ rgb2hex([255, 0, 0]) | from_json }}
 #    Returns: "#ff0000"
 #
 #    {{ rgb2hex([0, 255, 0]) | from_json }}
 #    Returns: "#00ff00"
-#}
{%- macro rgb2hex(rgb) -%}
  {%- set r = '%02x'|format(rgb[0]) -%}
  {%- set g = '%02x'|format(rgb[1]) -%}
  {%- set b = '%02x'|format(rgb[2]) -%}
  {{- ('#' + r + g + b) | to_json -}}
{%- endmacro -%}

{#-
 #  Convert hex colour to HSV (Hue, Saturation, Value)
 #
 #  Parameters:
 #    hex: Hex colour string (with or without #)
 #
 #  Returns:
 #    JSON array [h, s, v] where:
 #      h = hue (0-360 degrees)
 #      s = saturation (0-100 percent)
 #      v = value/brightness (0-100 percent)
 #
 #  Examples:
 #    {{ hex2hsv("#FF0000") | from_json }}
 #    Returns: [0, 100, 100] (pure red)
 #
 #    {{ hex2hsv("#008080") | from_json }}
 #    Returns: [180, 100, 50] (teal)
-#}
{%- macro hex2hsv(hex) -%}
  {%- set (ri,gi,bi) = hex2rgb(hex) | from_json -%}
  {%- set (r,g,b) = [ri|int(0)/255, gi|int(0)/255, bi|int(0)/255] -%}

  {%- set mn = min([r,g,b]) -%}
  {%- set mx = max([r,g,b]) -%}
  {%- set d = mx - mn -%}

  {%- set h = 0 -%}
  {%- set s = 0 -%}
  {%- set v = mx -%}

  {%- if d > 0 -%}
    {%- if r >= g and r >= b -%}
      {%- set h = (g - b) / d -%}
      {%- if h < 0 -%}{%- set h = h + 6 -%}{%- endif -%}
    {%- elif g >= b -%}
      {%- set h = 2 + ((b - r) / d) -%}
    {%- else -%}
      {%- set h = 4 + ((r - g) / d) -%}
    {%- endif -%}
    {%- set h = h / 6 -%}
    {%- set s = d / v -%}
  {%- endif -%}

  {%- set H = (h * 360) -%}
  {%- if H >= 360 -%}{%- set H = 0 -%}{%- endif -%}

  {{- [
      H | round(3),
      (s * 100) | round(3),
      (v * 100) | round(3)
    ] | to_json -}}
{%- endmacro -%}

{#-
 #  Convert hex colour to HS (Hue, Saturation only)
 #
 #  Parameters:
 #    hex: Hex colour string (with or without #)
 #
 #  Returns:
 #    JSON array [h, s] where:
 #      h = hue (0-360 degrees)
 #      s = saturation (0-100 percent)
 #
 #  Examples:
 #    {{ hex2hs("#FF0000") | from_json }}
 #    Returns: [0, 100]
 #
 #    {{ hex2hs("#FF8080") | from_json }}
 #    Returns: [0, 50] (light red has same hue, less saturation)
-#}
{%- macro hex2hs(hex) -%}
  {%- set (h,s,v) = hex2hsv(hex) | from_json -%}
  {{- [h,s] | to_json -}}
{%- endmacro -%}

{#-
 #  Convert hex colour to H (Hue only)
 #
 #  Parameters:
 #    hex: Hex colour string (with or without #)
 #
 #  Returns:
 #    JSON array [h] where h = hue (0-360 degrees).
 #
 #  Examples:
 #    {{ hex2h("#FF0000") | from_json }}
 #    Returns: [0]
 #
 #    {{ hex2h("#00FF00") | from_json }}
 #    Returns: [120]
-#}
{%- macro hex2h(hex) -%}
  {%- set (h,s,v) = hex2hsv(hex) | from_json -%}
  {{- [h] | to_json -}}
{%- endmacro -%}

{#-
 #  Convert hex colour to HSL (Hue, Saturation, Lightness)
 #
 #  Parameters:
 #    hex: Hex colour string (with or without #)
 #
 #  Returns:
 #    JSON array [h, s, l] where:
 #      h = hue (0-360 degrees)
 #      s = saturation (0-100 percent)
 #      l = lightness (0-100 percent, 50=pure colour)
 #
 #  Examples:
 #    {{ hex2hsl("#FF0000") | from_json }}
 #    Returns: [0, 100, 50]
 #
 #    {{ hex2hsl("#FF8080") | from_json }}
 #    Returns: [0, 100, 75] (lighter red)
-#}
{%- macro hex2hsl(hex) -%}
  {%- set (ri,gi,bi) = hex2rgb(hex) | from_json -%}
  {%- set (r,g,b) = [ri|int(0)/255, gi|int(0)/255, bi|int(0)/255] -%}

  {%- set mn = min([r,g,b]) -%}
  {%- set mx = max([r,g,b]) -%}
  {%- set d = mx - mn -%}
  {%- set l = (mx + mn) / 2 -%}

  {%- set h = 0 -%}
  {%- set s = 0 -%}

  {%- if d > 0 -%}
    {# hue (avoid float equality) #}
    {%- if r >= g and r >= b -%}
      {%- set h = (g - b) / d -%}
      {%- if h < 0 -%}{%- set h = h + 6 -%}{%- endif -%}
    {%- elif g >= b -%}
      {%- set h = 2 + ((b - r) / d) -%}
    {%- else -%}
      {%- set h = 4 + ((r - g) / d) -%}
    {%- endif -%}
    {%- set h = h / 6 -%}

    {# saturation: s = d / (1 - |2l - 1|) #}
    {%- set denom = 1 - (2*l - 1) | abs -%}
    {%- if denom > 0 -%}
      {%- set s = d / denom -%}
    {%- else -%}
      {%- set s = 0 -%}
    {%- endif -%}
  {%- endif -%}

  {%- set H = (h * 360) -%}
  {%- if H >= 360 -%}{%- set H = 0 -%}{%- endif -%}

  {{- [
      H | round(3),
      (s * 100) | round(3),
      (l * 100) | round(3)
    ] | to_json -}}
{%- endmacro -%}

{#-
 #  Convert hex colour to XYB (CIE colour space for HA)
 #
 #  Parameters:
 #    hex: Hex colour string (with or without #)
 #
 #  Returns:
 #    JSON array [x, y, brightness] where:
 #      x = CIE x coordinate (0-1)
 #      y = CIE y coordinate (0-1)
 #      brightness = brightness value (0-255)
 #    Used by some Home Assistant light integrations.
 #
 #  Examples:
 #    {{ hex2xyb("#FF0000") | from_json }}
 #    Returns: [0.701, 0.299, 54.213]
-#}
{%- macro hex2xyb(hex) -%}
  {%- set (r,g,b) = hex2rgb(hex) | from_json -%}
  {%- set (r,g,b) = [r|int(0)/255, g|int(0)/255, b|int(0)/255] -%}
  {%- if r == 0 and g == 0 and b == 0 -%}
    {%- set (x,y,Br) = [0.0,0.0,0] -%}
  {%- else -%}
    {%- if r > 0.04045 -%}
      {%- set r = ((r + 0.055) / 1.055) ** 2.4 -%}
    {%- else -%}
      {%- set r = r / 12.92 -%}
    {%- endif -%}

    {%- if g > 0.04045 -%}
      {%- set g = ((g + 0.055) / 1.055) ** 2.4 -%}
    {%- else -%}
      {%- set g = g / 12.92 -%}
    {%- endif -%}

    {%- if b > 0.04045 -%}
      {%- set b = ((b + 0.055) / 1.055) ** 2.4 -%}
    {%- else -%}
      {%- set b = b / 12.92 -%}
    {%- endif -%}
    {%- set X = r*0.664511+g*0.154324+b*0.162028 %}
    {%- set Y = r*0.283881+g*0.668433+b*0.047685 %}
    {%- set Z = r*0.000088+g*0.072310+b*0.986039 %}
    {%- set s = X + Y + Z -%}
    {%- if s <= 0 -%}
      {%- set (x, y, Br) = [0.0, 0.0, 0] -%}
    {%- else -%}
      {%- set x = X / s %}
      {%- set y = Y / s %}
      {%- if x < 0 -%}
        {%- set x = 0 -%}
      {%- elif x > 1 -%}
        {%- set x = 1 -%}
      {%- endif -%}
      {%- if y < 0 -%}
        {%- set y = 0 -%}
      {%- elif y > 1 -%}
        {%- set y = 1 -%}
      {%- endif -%}
      {%- set Br = (Y*255)|round(0)|int -%}
      {%- if Br < 0 -%}
        {%- set Br = 0 -%}
      {%- elif Br > 255 -%}
        {%- set Br = 255 -%}
      {%- endif -%}
    {%- endif -%}
  {%- endif -%}
  {{- [
      x | round(3),
      y | round(3),
      Br ] | to_json -}}
{%- endmacro -%}

{#-
 #  Convert HSV colour to RGB
 #
 #  Parameters:
 #    hsv: HSV array [h, s, v] where:
 #         h = hue (0-360 degrees)
 #         s = saturation (0-100 percent)
 #         v = value/brightness (0-100 percent)
 #
 #  Returns:
 #    JSON array [r, g, b] with values 0-255.
 #
 #  Examples:
 #    {{ hsv2rgb([0, 100, 100]) | from_json }}
 #    Returns: [255, 0, 0] (pure red)
 #
 #    {{ hsv2rgb([120, 100, 50]) | from_json }}
 #    Returns: [0, 128, 0] (green)
-#}
{%- macro hsv2rgb(hsv) -%}
  {%- set h = hsv[0] -%}
  {%- set s = hsv[1] / 100.0 -%}
  {%- set v = hsv[2] / 100.0 -%}

  {%- set c = v * s -%}
  {%- set x = c * (1 - ((h / 60.0) % 2 - 1) | abs) -%}
  {%- set m = v - c -%}

  {%- if h < 60 -%}
    {%- set rgb_prime = [c, x, 0] -%}
  {%- elif h < 120 -%}
    {%- set rgb_prime = [x, c, 0] -%}
  {%- elif h < 180 -%}
    {%- set rgb_prime = [0, c, x] -%}
  {%- elif h < 240 -%}
    {%- set rgb_prime = [0, x, c] -%}
  {%- elif h < 300 -%}
    {%- set rgb_prime = [x, 0, c] -%}
  {%- else -%}
    {%- set rgb_prime = [c, 0, x] -%}
  {%- endif -%}

  {%- set r = ((rgb_prime[0] + m) * 255) | round(0) | int -%}
  {%- set g = ((rgb_prime[1] + m) * 255) | round(0) | int -%}
  {%- set b = ((rgb_prime[2] + m) * 255) | round(0) | int -%}

  {{- [r, g, b] | to_json -}}
{%- endmacro -%}

{#-
 #  Convert HSV colour to hex
 #
 #  Parameters:
 #    hsv: HSV array [h, s, v] where:
 #         h = hue (0-360 degrees)
 #         s = saturation (0-100 percent)
 #         v = value/brightness (0-100 percent)
 #
 #  Returns:
 #    JSON string with hex colour (#RRGGBB) in lowercase.
 #
 #  Examples:
 #    {{ hsv2hex([0, 100, 100]) | from_json }}
 #    Returns: "#ff0000" (pure red)
 #
 #    {{ hsv2hex([240, 100, 50]) | from_json }}
 #    Returns: "#000080" (navy blue)
-#}
{%- macro hsv2hex(hsv) -%}
  {%- set rgb = hsv2rgb(hsv) | from_json -%}
  {{- rgb2hex(rgb) -}}
{%- endmacro -%}

