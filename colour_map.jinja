{#
from 'colour_map.jinja' import 
  random_colour, name_to_tag, tag_to_name, lookup_colour, find_colour,
  hex2rgb, hex2hsv, hex2hsl, hex2hs, hex2xyb, closest_ha_colour
#}

{# Random colour #}
{%- macro random_colour(exclude = [], map = "lamp") -%}
  {%- if map == "ha" -%}
    {%- set search = ha_colours -%}
  {%- else -%}
    {%- set search = lamp_colours -%}
  {%- endif -%}
  {%- set exclude = exclude | map('regex_replace', '\W', '', false) -%}
  {{ search.keys() | reject('in', exclude) | list | random }}
{%- endmacro -%}

{# Name crunching #}
{%- macro name_to_tag(name) -%}
  {{- name | regex_replace('\W','',false) -}}
{%- endmacro -%}

{%- macro tag_to_name(tag) -%}
  {{- tag | regex_replace('([a-z])([A-Z])', '\\1 \\2',false) -}}
{%- endmacro -%}

{# Data table extraction #}
{%- macro lookup_colour(id = random_colour(), attr="name", map = "lamp") -%}
  {%- if map == "ha" -%}
    {%- set search = ha_colours -%}
  {%- else -%}
    {%- set search = lamp_colours -%}
  {%- endif -%}
  {%- set id = find_colour(id, map) -%}
  {%- if id|length -%}
    {%- if attr == "rgb" -%}
      {{- hex2rgb(search[id]) -}}
    {%- elif attr == "hsv" -%}
      {{- hex2hsv(search[id]) -}}
    {%- elif attr == "hs" -%}
      {{- hex2hs(search[id]) -}}
    {%- elif attr == "hsl" -%}
      {{- hex2hsl(search[id]) -}}
    {%- elif attr == "xyb" -%}
      {{- hex2xyb(search[id]) -}}
    {%- elif attr == "hex" -%}
      {{- search[id] -}}
    {%- else -%}
      {{- tag_to_name(id) -}}
    {%- endif -%}
  {%- endif -%}
{%- endmacro -%}

{%- macro find_colour(name = "Cyan", map = "lamp") -%}
  {%- set tag = name_to_tag(name) -%}
  {%- if map == "ha" -%}
    {%- set search = ha_colours -%}
  {%- else -%}
    {%- set search = lamp_colours -%}
  {%- endif -%}
  {%- if search[tag] is defined -%}
    {{- tag -}}
  {%- else -%}
    {%- for i in search -%}
      {%- if i|lower == tag|lower -%}
        {{- i -}}
        {%- break -%}
      {%- endif -%}
    {%- endfor -%}
  {%- endif -%}
{%- endmacro -%}

{# Rough closest match #}
{%- macro _reduce_colour(hex) -%}
  {%- set (r,g,b) = hex2rgb(hex).split(',') -%}
  {{ (2*r|int*r|int) + (4*g|int*g|int) + (3*b|int*b|int) }}
{%- endmacro -%}

{%- macro closest_ha_colour(id = random_colour(), map = "lamp") -%}
  {%- if map == "ha" -%}
    {%- set search = ha_colours -%}
  {%- else -%}
    {%- set search = lamp_colours -%}
  {%- endif -%}
  {%- set nc = namespace(id="", delta=9999999,
    target=(_reduce_colour(lookup_colour(id, "hex", map)))|int(0)) -%}
  {%- for i in ha_colours -%}
    {%- set d = (nc.target - _reduce_colour(ha_colours[i])|int(0))|abs -%}
    {%- if d == 0 -%}
      {%- set nc.id = i -%}
      {%- break -%}
    {%- else -%}
      {%- if d < nc.delta -%}
        {%- set nc.id = i -%}
        {%- set nc.delta = d -%}
      {%- endif -%}
    {%- endif -%}
  {%- endfor -%}
  {{ nc.id }}
{%- endmacro -%}

{# Colour manipulation macros #}
{%- macro hex2rgb(hex) -%}
  {%- set hex = hex.replace('#','') -%}
  {{- hex[0:2]|int(0,16)|string + ',' 
    + hex[2:4]|int(0,16)|string + ','
    + hex[4:6]|int(0,16)|string -}}
{%- endmacro -%}

{%- macro hex2hsv(hex) -%}
  {%- set (r,g,b) = hex2rgb(hex).split(',') -%}
  {%- set (r,g,b) = [r|int(0)/255, g|int(0)/255, b|int(0)/255] -%}
  {%- set min = min([r,g,b]) -%}
  {%- set max = max([r,g,b]) -%}
  {%- set delta = max-min -%}
  {%- set h = 0 -%}
  {%- set s = 0 -%}
  {%- set v = max -%}
  {%- if delta>0 -%}
    {%- if max == r -%}
      {%- set h = ((g-b)/delta) -%}
      {%- if g<b -%}
        {%- set h = h+6 -%}
      {%- endif -%}
    {%- elif max == g -%}
      {%- set h = (2+((b-r)/delta)) -%}
    {%- elif max == b -%}
      {%- set h = (4+((r-g)/delta)) -%}
    {%- endif -%}
    {%- set h = h/6 -%}
    {%- set s = delta / v -%}
  {%- endif -%}
  {{-
    (h*360) | round(3) | string + ','
    + (s*100) | round(3) | string + ','
    + (v*100) | round(3) | string
  -}}
{%- endmacro -%}

{%- macro hex2hs(hex) -%}
  {%- set (h,s,v) = hex2hsv(hex).split(',') -%}
  {{- h + ',' + s -}}
{%- endmacro -%}

{%- macro hex2hsl(hex) -%}
  {%- set (r,g,b) = hex2rgb(hex).split(',') -%}
  {%- set (r,g,b) = [r|int(0)/255, g|int(0)/255, b|int(0)/255] -%}
  {%- set min = min([r,g,b]) -%}
  {%- set max = max([r,g,b]) -%}
  {%- set delta = max-min -%}
  {%- set l = (max+min)/2 -%}
  {%- set h = 0 -%}
  {%- set s = 0 -%}
  {%- if l==0 or l==1 -%}
    {%- set s = 0 -%}
  {%- elif l>0.5 -%}
    {%- set s = delta/(2-max-min) -%}
  {%- else -%}
    {%- set s = delta/(max+min) -%}
  {%- endif -%}
  {%- if delta>0 -%}
    {%- if max == r -%}
      {%- set h = ((g-b)/delta) -%}
      {%- if g<b -%}
        {%- set h = h+6 -%}
      {%- endif -%}
    {%- elif max == g -%}
      {%- set h = (2+((b-r)/delta)) -%}
    {%- elif max == b -%}
      {%- set h = (4+((r-g)/delta)) -%}
    {%- endif -%}
    {%- set h = h/6 -%}
  {%- endif -%}
  {{-
    (h*360) | round(3) | string + ','
    + (s*100) | round(3) | string + ','
    + (l*100) | round(3) | string
  -}}
{%- endmacro -%}

{%- macro hex2xyb(hex) -%}
  {%- set (r,g,b) = hex2rgb(hex).split(',') -%}
  {%- set (r,g,b) = [r|int(0)/255, g|int(0)/255, b|int(0)/255] -%}
  {%- if r == 0 and g == 0 and b == 0 -%}
    {%- set (x,y,br) = [0.0,0.0,0] -%}
  {%- else -%}
    {%- if r>0.04045 -%}
      {%- set r = ((r+0.055)/(1.055)**2.4) -%}
    {%- else -%}
      {%- set r = (r/12.92) -%}
    {%- endif -%}
    {%- if (g>0.04045) %}
      {%- set g = ((g+0.055)/(1.055)**2.4) %}
    {%- else %}
      {%- set g = (g/12.92) %}
    {%- endif %}
    {%- if (b>0.04045) %}
      {%- set b = ((b+0.055)/(1.055)**2.4) %}
    {%- else %}
      {%- set b = (b/12.92) %}
    {%- endif %}
    {%- set x = r*0.664511+g*0.154324+b*0.162028 %}
    {%- set y = r*0.283881+g*0.668433+b*0.047685 %}
    {%- set z = r*0.000088+g*0.072310+b*0.986039 %}
    {%- set x = x/(x+y+z) %}
    {%- set y = y/(x+y+z) %}
    {%- if y > 1 -%}
      {%- set y = 1 -%}
    {%- endif -%}
    {%- set Br = (y*255)|round(3) -%}
  {%- endif -%}
  {{- x | round(3) | string + ','
    + y | round(3) | string + ','
    + Br | string
  -}}
{%- endmacro -%}

