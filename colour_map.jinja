{# Phil: Macros for colour names #}
{# NB: Macros return strings, so results are json format #}
{# LA-HA v2.0 #}

{#
from 'colours.jinja' import 
  random_colour, random_colours,
  name_to_tag, tag_to_name,
  lookup_colour, lookup_colours, find_colour,
  hex2rgb, hex2hsv, hex2hsl, hex2hs, hex2h,
  hex2xyb, rgb2hex
#}

{# Random colour #}
{# Parameters:
   - map: Colour name map to use
#}
{%- macro random_colour(map="lamp") -%}
  {%- set colour_map = colours[map] -%}
  {{- colour_map.keys() | list | random | to_json -}}
{%- endmacro -%}

{# Random colours #}
{# Parameters:
   - n: Number of colours to return
   - map: Colour name map to use
#}
{%- macro random_colours(n=1, map="lamp") -%}
  {%- set colour_map = colours[map] -%}
  {%- set search = colour_map.keys() | list -%}
  {%- if n > (search | count) -%}
    {%- set n = search | count -%}
  {%- endif -%}
  {%- set e = namespace(ex = [], ac = []) -%}
  {%- for i in range(n) -%}
    {%- set available = search | reject('in', e.ex) | list -%}
    {%- if (available | count) == 0 -%}
      {# Pool exhausted, break early #}
      {%- break -%}
    {%- elif (available | count) <= (n - (e.ac | count)) -%}
      {# Remaining pool fits in what we need, take them all #}
      {%- set e.ac = e.ac + available -%}
      {%- break -%}
    {%- else -%}
      {# Pool has more than we need, pick one randomly #}
      {%- set c = available | random -%}
      {%- set e.ex = e.ex + [c] -%}
      {%- set e.ac = e.ac + [c] -%}
    {%- endif -%}
  {%- endfor -%}
  {{- e.ac | to_json -}}
{%- endmacro -%}

{# Name crunching #}
{# Parameters:
   - name/tag: Colour name/tag to turn into a tag/name
#}
{%- macro name_to_tag(name) -%}
  {{- name | regex_replace('\W','',false) | to_json -}}
{%- endmacro -%}

{%- macro tag_to_name(tag) -%}
  {{- tag | regex_replace('([a-z])([A-Z])', '\\1 \\2',false) | to_json -}}
{%- endmacro -%}

{# Data table extraction #}
{# Parameters:
   - id/cs: Colour tag, name or comma seperated list of colour names or tags
   - attr: Which attribute to return
   - map: Which colour map to use
#}
{%- macro lookup_colour(id=None, attr="name", map="lamp") -%}
  {%- set colour_map = colours[map] -%}
  {%- if id is none or id == "random" -%}
    {%- set id = random_colour(map) | from_json -%}
  {%- else -%}
    {%- set id = find_colour(id, map) | from_json -%}
  {%- endif -%}
  {%- if id|length -%}
    {%- if attr == "rgb" -%}
      {{- hex2rgb(colour_map[id]) -}}
    {%- elif attr == "hsv" -%}
      {{- hex2hsv(colour_map[id]) -}}
    {%- elif attr == "hs" -%}
      {{- hex2hs(colour_map[id]) -}}
    {%- elif attr == "h" -%}
      {{- hex2h(colour_map[id]) -}}
    {%- elif attr == "hsl" -%}
      {{- hex2hsl(colour_map[id]) -}}
    {%- elif attr == "xyb" -%}
      {{- hex2xyb(colour_map[id]) -}}
    {%- elif attr == "hex" -%}
      {{- ('#' + colour_map[id] | lower) | to_json -}}
    {%- else -%}
      {{- tag_to_name(id) -}}
    {%- endif -%}
  {%- endif -%}
{%- endmacro -%}

{%- macro lookup_colours(cs=None, attr="name", map="lamp") -%}
  {%- set r = namespace(c=[]) -%}
  {%- if cs is none -%}
    {%- set cs = random_colour(map) | from_json -%}
  {%- endif -%}
  {%- if cs is string -%}
    {%- set cs = (cs | replace(' and ', ',')).split(',') |
        map('trim') | list -%}
  {%- endif -%}
  {%- for i in cs -%}
    {%- set lu = lookup_colour(i, attr, map) -%}
    {%- if lu -%}
      {%- set r.c = r.c + [lu | from_json] -%}
    {%- endif -%}
  {%- endfor -%}
  {{- r.c | to_json -}}
{%- endmacro -%}

{%- macro find_colour(name=None, map="lamp") -%}
  {%- set colour_map = colours[map] -%}
  {%- if name is none -%}
    {%- set name = random_colour(map) | from_json -%}
  {%- endif -%}
  {%- set tag = name_to_tag(name) | from_json -%}
  {%- if colour_map[tag] is defined -%}
    {{- tag | to_json -}}
  {%- else -%}
    {%- set tag = tag|lower -%}
    {%- for i in colour_map.keys() | list -%}
      {%- if i|lower == tag -%}
        {{- i | to_json -}}
        {%- break -%}
      {%- endif -%}
    {%- endfor -%}
  {%- endif -%}  
{%- endmacro -%}

{# Colour manipulation macros #}
{%- macro hex2rgb(hex) -%}
  {%- set hex = hex.replace('#','') -%}
  {{- [
      hex[0:2]|int(0,16),
      hex[2:4]|int(0,16),
      hex[4:6]|int(0,16) ] | to_json -}}
{%- endmacro -%}

{%- macro rgb2hex(rgb) -%}
  {%- set r = '%02x'|format(rgb[0]) -%}
  {%- set g = '%02x'|format(rgb[1]) -%}
  {%- set b = '%02x'|format(rgb[2]) -%}
  {{- ('#' + r + g + b) | to_json -}}
{%- endmacro -%}

{%- macro hex2hsv(hex) -%}
  {%- set (r,g,b) = hex2rgb(hex) | from_json -%}
  {%- set (r,g,b) = [r|int(0)/255, g|int(0)/255, b|int(0)/255] -%}
  {%- set min = min([r,g,b]) -%}
  {%- set max = max([r,g,b]) -%}
  {%- set delta = max-min -%}
  {%- set h = 0 -%}
  {%- set s = 0 -%}
  {%- set v = max -%}
  {%- if delta>0 -%}
    {%- if max == r -%}
      {%- set h = ((g-b)/delta) -%}
      {%- if g<b -%}
        {%- set h = h+6 -%}
      {%- endif -%}
    {%- elif max == g -%}
      {%- set h = (2+((b-r)/delta)) -%}
    {%- elif max == b -%}
      {%- set h = (4+((r-g)/delta)) -%}
    {%- endif -%}
    {%- set h = h/6 -%}
    {%- set s = delta / v -%}
  {%- endif -%}
  {{- [ 
      (h*360) | round(3),
      (s*100) | round(3),
      (v*100) | round(3) ] | to_json -}}
{%- endmacro -%}

{%- macro hex2hs(hex) -%}
  {%- set (h,s,v) = hex2hsv(hex) | from_json -%}
  {{- [h,s] | to_json -}}
{%- endmacro -%}

{%- macro hex2h(hex) -%}
  {%- set (h,s,v) = hex2hsv(hex) | from_json -%}
  {{- [h] | to_json -}}
{%- endmacro -%}

{%- macro hex2hsl(hex) -%}
  {%- set (r,g,b) = hex2rgb(hex) | from_json -%}
  {%- set (r,g,b) = [r|int(0)/255, g|int(0)/255, b|int(0)/255] -%}
  {%- set min = min([r,g,b]) -%}
  {%- set max = max([r,g,b]) -%}
  {%- set delta = max-min -%}
  {%- set l = (max+min)/2 -%}
  {%- set h = 0 -%}
  {%- set s = 0 -%}
  {%- if l==0 or l==1 -%}
    {%- set s = 0 -%}
  {%- elif l>0.5 -%}
    {%- set s = delta/(2-max-min) -%}
  {%- else -%}
    {%- set s = delta/(max+min) -%}
  {%- endif -%}
  {%- if delta>0 -%}
    {%- if max == r -%}
      {%- set h = ((g-b)/delta) -%}
      {%- if g<b -%}
        {%- set h = h+6 -%}
      {%- endif -%}
    {%- elif max == g -%}
      {%- set h = (2+((b-r)/delta)) -%}
    {%- elif max == b -%}
      {%- set h = (4+((r-g)/delta)) -%}
    {%- endif -%}
    {%- set h = h/6 -%}
  {%- endif -%}
  {{- [
      (h*360) | round(3),
      (s*100) | round(3),
      (l*100) | round(3) ] | to_json -}}
{%- endmacro -%}

{%- macro hex2xyb(hex) -%}
  {%- set (r,g,b) = hex2rgb(hex) | from_json -%}
  {%- set (r,g,b) = [r|int(0)/255, g|int(0)/255, b|int(0)/255] -%}
  {%- if r == 0 and g == 0 and b == 0 -%}
    {%- set (x,y,br) = [0.0,0.0,0] -%}
  {%- else -%}
    {%- if r>0.04045 -%}
      {%- set r = ((r+0.055)/(1.055)**2.4) -%}
    {%- else -%}
      {%- set r = (r/12.92) -%}
    {%- endif -%}
    {%- if (g>0.04045) %}
      {%- set g = ((g+0.055)/(1.055)**2.4) %}
    {%- else %}
      {%- set g = (g/12.92) %}
    {%- endif %}
    {%- if (b>0.04045) %}
      {%- set b = ((b+0.055)/(1.055)**2.4) %}
    {%- else %}
      {%- set b = (b/12.92) %}
    {%- endif %}
    {%- set x = r*0.664511+g*0.154324+b*0.162028 %}
    {%- set y = r*0.283881+g*0.668433+b*0.047685 %}
    {%- set z = r*0.000088+g*0.072310+b*0.986039 %}
    {%- set x = x/(x+y+z) %}
    {%- set y = y/(x+y+z) %}
    {%- if y > 1 -%}
      {%- set y = 1 -%}
    {%- endif -%}
    {%- set Br = (y*255)|round(3) -%}
  {%- endif -%}
  {{- [
      x | round(3),
      y | round(3),
      Br ] | to_json -}}
{%- endmacro -%}

